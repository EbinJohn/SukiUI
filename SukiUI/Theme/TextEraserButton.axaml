<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SukiUI.Theme.TextEraserButton">
    <Button Click="Button_OnClick"  Width="25" Height="25" Padding="0" Margin="5,0,0,0" BorderThickness="0" Classes="Void" HorizontalAlignment="Center" VerticalAlignment="Center" >
                                                
        <avalonia:MaterialIcon Margin="0" Height="20" Width="20" Kind="CloseCircle" Foreground="{DynamicResource SukiControlBorderBrush}"></avalonia:MaterialIcon>
    </Button>
</UserControl>
